{
    "name": "tesseract/crypto-sdk-laravel",
    "description": "Tesseract Crypto SDK PHP Laravel from Tesseract Crypto API v2",
    "version": "0.2.6",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": ">=5.0,<5.8",
        "tesseract/crypto-sdk": "0.2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Jaramillo",
            "email": "cristian_gerar@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tesseract\\": "src/Tesseract"
        },
        "files": [

        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Tesseract\\Illuminate\\Support\\ServiceProvider"
            ],
            "aliases": {
                "CryptoSDK": "Tesseract\\Illuminate\\Support\\Facade"
            }
        }
    }
}
